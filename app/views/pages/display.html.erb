<% title @title %>

<p>
	<strong><%= @page.title %></strong>
</p>

<p>
	<%= h(@page.body) %>
</p>

	
<% if signed_in? %>
<p>
  <%= link_to "Edit", edit_page_path(@page) %> |
  <%= link_to "Destroy", @page, :confirm => 'Are you sure?', :method => :delete %> |
  <%= link_to "View All", pages_path %>
</p>
	
<% end %>